<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="comzan" catalog="shanhai"
		class="com.shanhai.model.Comzan">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="discussmake" catalog="shanhai"
		class="com.shanhai.model.Discussmake">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="ilike" catalog="shanhai"
		class="com.shanhai.model.Ilike">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="moment" catalog="shanhai"
		class="com.shanhai.model.Moment">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="user" catalog="shanhai" class="com.shanhai.model.User">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>